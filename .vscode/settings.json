    "workbench.editor.enablePreview": false,

    "editor.multiCursorModifier": "alt",

    // remove trailing whitespace( extra spaces from start and end)
    "files.trimTrailingWhitespace": true,

    "diffEditor.renderSideBySide": false,

    // In suggesion snippet shows first. then other suggestion such as keywords, etc..
    "editor.snippetSuggestions": "top",

    // based on file connect tab size and whitesapce size change if true, if false then not change
    "editor.detectIndentation": false,

    // after saving add new line to the file
        // make sure this setting is after  "files.trimTrailingWhitespace" otherwise lastly added line will also be trimmed
    "files.insertFinalNewline": true,

    // When enabled, will trim all new lines after the final new line at the end of the file when saving it.
    "files.trimFinalNewlines": true,

    // remove tabs from working area
    // "workbench.editor.showTabs": "none",

    // vs code will not give indentation related guide
    "editor.guides.indentation": false,

    // after commit you will not see the green lines after add, red lines after delete, blue after modify
    "scm.diffDecorations": "none",

    // When Hower, after 1500 milisecond something will show
    "editor.hover.delay": 1500,

    // after hover on function tooltips will show above the function's line
    // if false then tooltip pop-up will show below the function line
    "editor.hover.above": true,

    // show function arguments(signature) after hover
    "editor.hover.enabled": true,

    // if setted to "always" then it shows square on bracket
    "editor.matchBrackets": "never",

    // if your profile is web devlopment and you have .py file then vs code suggest you to download python extemsion pack that thing
    "workbench.tips.enabled": false,

    // show a square color preview box
    "editor.colorDecorators": true,


    // visually decorate git related chnages like add,update,delete
    "git.decorations.enabled": false,

    // Controls which editor is shown at startup, if none are restored from the previous session.
    "workbench.startupEditor": "none",

    "editor.lightbulb.enabled": "off",

    // after selecting some text not highlight it's other occurancies ( Prevent automatic highlight )
    "editor.selectionHighlight": false,

    // same as above ( prevent highlight when using cursor we select something and other occurancies also highlight )
    "editor.occurrencesHighlight": "off",

    // at right side of the window we have scroll bar that shows colors for error(red), warning(yellow)
    "editor.overviewRulerBorder": false,

    // highlisht that line in which cursor is present
    "editor.renderLineHighlight": "none",

    // incate error with hightlight as wavy red underline
    "problems.decorations.enabled": false,

    // When set to true, control characters will be rendered as visible symbols (like ↵ for newlines or ⇥ for tabs),
    "editor.renderControlCharacters": true,

    // in right sidebar cursor(|) postion will shown as a line if setted false. not shown if setted true
    "editor.hideCursorInOverviewRuler": false,


    "editor.gotoLocation.multipleReferences": "goto",
    "editor.gotoLocation.multipleDeclarations": "goto",
    "editor.gotoLocation.multipleDefinitions": "goto",

    // preview mode can be invoke by pressing Ctrl + p
